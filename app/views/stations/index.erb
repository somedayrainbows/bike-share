<h1> All Bike Stations </h1>
<table>
  <tr>
    <th>Name</th>
    <th>Dock Count</th>
    <th>City</th>
    <th>Installation Date</th>
  </tr>

<% @stations.each do |station| %>
     <tr class="station-info">
       <td> <%= station.name %> </td>
       <td> <%= station.dock_count %> </td>
       <td> <%= station.city %> </td>
       <td> <%= station.installation_date %> </td>
    </tr>
<% end %>
</table>
